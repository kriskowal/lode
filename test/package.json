{
    "lode": true,
    "main": "main.js",
    "public": [""],
    "includes": [
        "includes/bar",
        "includes/baz"
    ],
    "mappings": {
        "foo": "mappings/foo",
        "qux": {"archive": "mappings/qux.zip"}
    },
    "scope": [
        {"capability": "console"},
        {"capability": "node@0.4.0"}
    ],
    "loaders": [
        {"extension": ""},
        {"extension": ".js"},
        {"extension": ".coffee", "package": {
            "npm": "coffee-script"
        }}
    ],
    "registry": "http://npm",
    "supportDefine": true
}
