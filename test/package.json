{
    "lode": true,
    "main": "main.js",
    "public": [""],
    "includes": [
        "includes/bar",
        "includes/baz"
    ],
    "mappings": {
        "foo": "mappings/foo",
        "qux": {"archive": "mappings/qux.zip"},
        "package": {"capability": "package"}
    },
    "scope": [
        {"capability": "console"},
        {"capability": "node@0.4.0"}
    ],
    "loaders": [
        {
            "extension": ".coffee",
            "package": {"system": "coffee-script"}
        }
    ],
    "resources": [
        "data",
        "package.json"
    ],
    "supportDefine": true
}
